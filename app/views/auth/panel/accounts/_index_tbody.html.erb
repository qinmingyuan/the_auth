<td>
<% if model.user %>
  <%= link_to model.user.name, { controller: 'users', action: 'show', id: model.user_id }, text: true %>
<% end %>
</td>
<td><%= model.type_i18n %></td>
<td><%= model.identity %></td>
<td>
  <div data-controller="input">
    <%= check_box_tag '[model]confirmed', nil, model.confirmed, data: { method: :patch, url: panel_account_path(model), action: 'input#submit' }, id: nil, class: 'switch is-rounded' %>
    <label></label>
  </div>
</td>
<td>
  <div data-controller="input">
    <%= check_box_tag '[model]primary', nil, model.primary, data: { method: :patch, url: panel_account_path(model), action: 'input#submit' }, id: nil, class: 'switch is-rounded' %>
    <label></label>
  </div>
</td>
<td><time data-controller="time"><%= model.created_at.to_s(:rfc822) %></time></td>
